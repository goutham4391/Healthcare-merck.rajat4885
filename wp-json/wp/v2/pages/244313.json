      <div class=\"mco-b2-hero-block-image-container-two\">\n                <div class='mco-b2-hero-block-green-box'><\/div>            <\/div>\n        <\/div>\n\n        <div class=\"mco-b2-hero-block-content-section\">\n            <div class=\"breadcrumbs wrapper text-descriptive\"><\/div>            \n            <h1>Discovery &amp; development<\/h1>\n\n                            <div class=\"mco-b2-hero-block-description\"><p>We follow the science where we can make the greatest difference, now and in the future.<\/p>\n<p>We aspire to be the premier research-intensive biopharmaceutical company. We bring creative minds together to transform world-class science into life-changing medicines and vaccines.<\/p>\n<\/div>\n            \n                    <\/div>\n    <\/div>\n<\/div>\n\n<div class=\"mco-b2-hero-block-modal popup\" style=\"display: none;\">\n    <div class=\"popup-wrapper\">\n        <div class=\"mco-b2-hero-block-modal-content popup-content\">\n        <\/div>\n    <\/div>\n<\/div>\n\n<script>\n    let b2_hero_block_modal_triggers = document.getElementsByClassName('mco-b2-hero-block-toggle-btn');\n    let b2_hero_block_video_type = document.getElementsByClassName('mco-b2-hero-block-video-type');\n    let b2_hero_block_youtube_url = document.getElementsByClassName('mco-b2-hero-block-youtube-url');\n    let b2_hero_block_brightcove_info = document.getElementsByClassName('mco-b2-hero-block-brightcove-info');\n    let b2_hero_block_modals = document.getElementsByClassName('mco-b2-hero-block-modal');\n    for(let i = 0; i < b2_hero_block_modal_triggers.length; i++) {\n        let trigger = b2_hero_block_modal_triggers[i];\n        let video_type = b2_hero_block_video_type[i].textContent;\n        let youtube_url = b2_hero_block_youtube_url[i].textContent;\n        let brightcove_info = b2_hero_block_brightcove_info[i].textContent;\n        let modal = b2_hero_block_modals[i];\n        let modal_content_div = modal.getElementsByClassName('mco-b2-hero-block-modal-content')[0];\n        trigger.addEventListener('click', function () {\n            if (modal.style.display == \"none\") {\n                \/\/ Load Video Content\n                if (video_type == 'youtube') {\n                    \/\/ Insert YouTube Iframe into Div\n                    let youtube_video_id = youtube_url.split('=')[1].split('&#038;')[0]\n                    modal_content_div.innerHTML = `<iframe width='933' height='525' src='https:\/\/www.youtube.com\/embed\/${youtube_video_id}' frameborder='0' allow='accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture' allowfullscreen><\/iframe>`;\n                } else {\n                    \/\/ Insert Brightcove Video Info Into Div\n                    let account_id = brightcove_info.split(\":\")[0];\n                    let video_id = brightcove_info.split(\":\")[1];\n                    modal_content_div.innerHTML = `<video class='video-js vjs-fluid mco-featured-stories-e1-video' data-account='${account_id}' data-player='default' data-embed='default' controls='' data-video-id='${video_id}' data-playlist-id='' data-application-id='' width='960' height='540'><\/video>`;\n                    let brightcove_videos = document.getElementsByClassName(\"mco-featured-stories-e1-video\");\n                    for (let i = 0; i < brightcove_videos.length; i++) {\n                        let video = brightcove_videos[i];\n                        let script = document.createElement(\"script\");\n                        script.async = true;\n                        script.src = \"https:\/\/players.brightcove.net\/\" + account_id + \"\/default_default\/index.min.js\";\n                        video.parentNode.insertBefore(script, video.nextSibling);\n                    }\n                }\n            }\n            modal.style.display = modal.style.display == \"none\" ? \"block\" : \"none\";\n        });\n        modal.addEventListener('click', function(e) {\n            if (e.target.classList.contains(\"mco-b2-hero-block-modal\") || e.target.classList.contains('popup-wrapper')) {\n                modal.setAttribute('style', 'display: none;');\n            }\n        });\n    }\n<\/script>\n\n<div id=\"mco-featured-stories-e1-block-block_5e9f40c1966a7\" class=\"mco-featured-stories-e1-block full corporateberg-block\">\n    \n    <div class=\"mco-featured-stories-e1-content-container\">\n                            <div class=\"mco-featured-stories-e1-content-item-first\">\n                        <div class=\"mco-featured-stories-e1-content-item-first-image-container \">\n                            \n                                                                <a href=\"https:\/\/www.merck.com\/stories\/why-were-expanding-and-investing-in-our-research-and-discovery-efforts\/\">\n                                                                        <picture>\n                                        <source media=\"(max-width: 767px)\" data-srcset=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2022\/06\/commitment-to-innovation-featured-image.jpg?w=1024?resize=375,196\">\n                                        <source media=\"(min-width: 768px) and (max-width: 1279px)\" data-srcset=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2022\/06\/commitment-to-innovation-featured-image.jpg?w=1024?resize=656,399\">\n                                        <source media=\"(min-width: 1280px)\" data-srcset=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2022\/06\/commitment-to-innovation-featured-image.jpg?w=1024?resize=560,404\">\n                                        <img\n                                            data-src=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2022\/06\/commitment-to-innovation-featured-image.jpg?w=1024?resize=560,404\"\n                                            alt=\"Article thumbnail\"\n                                            class=\"lazyload\"\n                                            loading=\"lazy\"\n                                        \/>\n                                    <\/picture>\n                                                                            <\/a>\n                                                            <\/div>\n\n                        <div class=\"mco-featured-stories-e1-content-item-first-content-container\">\n                            <a href=\"https:\/\/www.merck.com\/stories\/why-were-expanding-and-investing-in-our-research-and-discovery-efforts\/\">\n                                                        <small class=\"tagline\">Innovation<\/small>\n                            <h4><span>Why we&#8217;re expanding and investing in our research and discovery efforts<\/span><\/h4>\n                            <p>Our commitment to innovation helps ensure a brighter future for patients everywhere<\/p>\n                            <\/a>\n                        <\/div>\n\n                    <\/div>\n                                    <div class=\"mco-featured-stories-e1-content-item\">\n                        \n                            <div class=\"mco-featured-stories-e1-content-item-content-container\">\n                                <a href=\"https:\/\/www.merck.com\/stories\/our-new-london-discovery-research-center-site\/\">\n                                                                        <small class=\"tagline\">Innovation<\/small>\n                                    <h4><span>Our new London Discovery Research Center site<\/span><\/h4>\n                                    <p>The $1.32 billion, 220,000-square-foot research facility will be located in London\u2019s \u201cKnowledge Quarter\u201d<\/p>\n                                <\/a>\n                            <\/div>\n\n                            <div class=\"mco-featured-stories-e1-content-item-image-container\">\n                                <a href=\"https:\/\/www.merck.com\/stories\/our-new-london-discovery-research-center-site\/\">\n                                                                                <picture>\n                                                <source media=\"(max-width: 767px)\" data-srcset=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2020\/08\/Discovery-Research-Center-hero.jpg?w=1024?resize=133,133\">\n                                                <source media=\"(min-width: 768px) and (max-width: 1279px)\" data-srcset=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2020\/08\/Discovery-Research-Center-hero.jpg?w=1024?resize=141,144\">\n                                                <source media=\"(min-width: 1280px)\" data-srcset=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2020\/08\/Discovery-Research-Center-hero.jpg?w=1024?resize=180,180\">\n                                                <img\n                                                    data-src=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2020\/08\/Discovery-Research-Center-hero.jpg?w=1024?resize=180,180\"\n                                                    alt=\"Article thumbnail\"\n                                                    class=\"lazyload\"\n                                                    loading=\"lazy\"\n                                                \/>\n                                            <\/picture>\n                                                                            <\/a>\n                            <\/div>\n                    <\/div>\n                                    <div class=\"mco-featured-stories-e1-content-item\">\n                        \n                            <div class=\"mco-featured-stories-e1-content-item-content-container\">\n                                <a href=\"https:\/\/www.merck.com\/stories\/merck-scientists-probe-how-our-bodies-bugs-impact-immunity-and-disease\/\">\n                                                                        <small class=\"tagline\">Innovation<\/small>\n                                    <h4><span>Merck scientists probe how our bodies&#8217; bugs impact immunity and disease<\/span><\/h4>\n                                    <p>Our researchers are pioneering new ways to research and understand the biology of new diseases by looking inside the human gut<\/p>\n                                <\/a>\n                            <\/div>\n\n                            <div class=\"mco-featured-stories-e1-content-item-image-container\">\n                                <a href=\"https:\/\/www.merck.com\/stories\/merck-scientists-probe-how-our-bodies-bugs-impact-immunity-and-disease\/\">\n                                                                                <picture>\n                                                <source media=\"(max-width: 767px)\" data-srcset=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2020\/10\/Scientists-RY-Preparing-slide-1450-1-HI.jpg?w=1024?resize=133,133\">\n                                                <source media=\"(min-width: 768px) and (max-width: 1279px)\" data-srcset=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2020\/10\/Scientists-RY-Preparing-slide-1450-1-HI.jpg?w=1024?resize=141,144\">\n                                                <source media=\"(min-width: 1280px)\" data-srcset=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2020\/10\/Scientists-RY-Preparing-slide-1450-1-HI.jpg?w=1024?resize=180,180\">\n                                                <img\n                                                    data-src=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2020\/10\/Scientists-RY-Preparing-slide-1450-1-HI.jpg?w=1024?resize=180,180\"\n                                                    alt=\"Article thumbnail\"\n                                                    class=\"lazyload\"\n                                                    loading=\"lazy\"\n                                                \/>\n                                            <\/picture>\n                                                                            <\/a>\n                            <\/div>\n                    <\/div>\n                                    <div class=\"mco-featured-stories-e1-content-item\">\n                        \n                            <div class=\"mco-featured-stories-e1-content-item-content-container\">\n                                <a href=\"https:\/\/www.merck.com\/stories\/women-in-science-absolutely\/\">\n                                                                        <small class=\"tagline\">Our people<\/small>\n                                    <h4><span>Women in science? Absolutely.<\/span><\/h4>\n                                    <p>Strong career paths and cutting-edge science draw more women to our small molecule process R&amp;D team <\/p>\n                                <\/a>\n                            <\/div>\n\n                            <div class=\"mco-featured-stories-e1-content-item-image-container\">\n                                <a href=\"https:\/\/www.merck.com\/stories\/women-in-science-absolutely\/\">\n                                                                                <picture>\n                                                <source media=\"(max-width: 767px)\" data-srcset=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2021\/02\/allFour.jpg?w=1024?resize=133,133\">\n                                                <source media=\"(min-width: 768px) and (max-width: 1279px)\" data-srcset=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2021\/02\/allFour.jpg?w=1024?resize=141,144\">\n                                                <source media=\"(min-width: 1280px)\" data-srcset=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2021\/02\/allFour.jpg?w=1024?resize=180,180\">\n                                                <img\n                                                    data-src=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2021\/02\/allFour.jpg?w=1024?resize=180,180\"\n                                                    alt=\"Article thumbnail\"\n                                                    class=\"lazyload\"\n                                                    loading=\"lazy\"\n                                                \/>\n                                            <\/picture>\n                                                                            <\/a>\n                            <\/div>\n                    <\/div>\n                    <\/div>\n    \n    <div class=\"mco-stock-ticker-hero-modal popup\" style=\"display: none;\">\n        <div class=\"popup-wrapper\">\n            <div class=\"mco-featured-stories-e1-modal-content popup-content\">\n                <h1>VIDEO COMING SOON<\/h1>\n            <\/div>\n        <\/div>\n    <\/div>\n<\/div>\n\n\n\n<div id=\"accordion_list_block_5e9f4439966a8\" class=\"b6-accordion-list corporateberg-block\">\n    \n        <h2>\n        <span>Our research &amp; development process<\/span>\n        <button class=\"btn tertiary\" onclick=\"expandAll_accordion_list_block_5e9f4439966a8()\">\n            <span class=\"open-text\">Expand all<\/span>\n            <span class=\"close-text\">Close all<\/span>\n        <\/button>\n    <\/h2>\n                            <div class=\"b6-accordion-list-item\">\n                <details class=\"b6-accordion-list-item-details\">\n                    <summary class=\"b6-accordion-list-item-summary\">\n                        <h5>Candidate discovery<\/h5>\n                    <\/summary>\n                    <div class=\"b6-accordion-list-item-content\">\n                        <div><p><strong>Our journey to discovering new medicines and vaccines starts by understanding the inner workings of a disease<\/strong><\/p>\n<p>We follow cutting-edge science to analyze diseases from every angle possible to accelerate the pace of drug discovery &#8211; from its underlying genetics, to the structure of individual proteins and the way hundreds of molecules interact with each other. Our scientists are using automation, miniaturization and information technologies as well as new tools &#8211; such as our cryo-electron microscope &#8211; to help us assess targets at a faster pace and design new medicines. In oncology, we are also evaluating tumor genetics and genomics to identify new targets.<\/p>\n<\/div>\n                                            <\/div>\n                <\/details>\n                <hr \/>\n            <\/div>\n                    <div class=\"b6-accordion-list-item\">\n                <details class=\"b6-accordion-list-item-details\">\n                    <summary class=\"b6-accordion-list-item-summary\">\n                        <h5>Preclinical development<\/h5>\n                    <\/summary>\n                    <div class=\"b6-accordion-list-item-content\">\n                        <div><p><strong>Before breakthroughs, there is science. We ensure only the most promising innovations with the greatest potential advance for future testing<\/strong><\/p>\n<p>Across the biopharmaceutical industry, it takes many scientists many years to design, make and test thousands of new molecules in order to find the one or two that may be tested in clinical trials. Each discovery is refined, optimized and carefully scrutinized before it can be tested in humans. Our scientists are using preclinical data to conduct translational research to identify biomarkers that can be used in the clinic to provide surrogate measures of efficacy for our investigational candidates.<\/p>\n<\/div>\n                                            <\/div>\n                <\/details>\n                <hr \/>\n            <\/div>\n                    <div class=\"b6-accordion-list-item\">\n                <details class=\"b6-accordion-list-item-details\">\n                    <summary class=\"b6-accordion-list-item-summary\">\n                        <h5>Clinical development<\/h5>\n                    <\/summary>\n                    <div class=\"b6-accordion-list-item-content\">\n                        <div><p><strong>Our success is due in large part to the challenging scientific questions we set out to answer with our trials and collaborations<\/strong><\/p>\n<p>Our researchers work internally &#8211; as well as with the medical community &#8211; to generate evidence to determine whether a medicine has potential to benefit patients. Once our investigational therapies are in clinical trials, our scientists implement machine learning technologies using artificial intelligence to analyze big data from the trials to help us understand the fundamentals of human biology and how that relates to disease modification.<\/p>\n<\/div>\n                                            <\/div>\n                <\/details>\n                <hr \/>\n            <\/div>\n                    <div class=\"b6-accordion-list-item\">\n                <details class=\"b6-accordion-list-item-details\">\n                    <summary class=\"b6-accordion-list-item-summary\">\n                        <h5>Real-world data<\/h5>\n                    <\/summary>\n                    <div class=\"b6-accordion-list-item-content\">\n                        <div><p><strong>Our Center for Observational and Real-World Evidence (CORE) is dedicated to the science of understanding how our medicines contribute and function in the real world<\/strong><\/p>\n<p>We believe that real-world evidence, or data acquired in everyday clinical practice, is an important complement to our rigorous clinical trial program. It allows us to take insights from real-world clinical practice back to our research labs to better understand our existing medicines and inform the discovery and development of future therapies.<\/p>\n<\/div>\n                                            <\/div>\n                <\/details>\n                <hr \/>\n            <\/div>\n            <\/div>\n<script>\n    function expandAll_accordion_list_block_5e9f4439966a8() {\n        let accordionList = document.getElementById('accordion_list_block_5e9f4439966a8');\n        let accordionListItems = accordionList.querySelectorAll('details');\n        if(!accordionList.classList.contains('all-expanded')) {\n            accordionList.classList.add('all-expanded');\n            Array.prototype.forEach.call(accordionListItems, function(accordionListItem, i){\n                accordionListItem.setAttribute('open', 'open');\n            });\n        } else {\n            accordionList.classList.remove('all-expanded');\n            Array.prototype.forEach.call(accordionListItems, function(accordionListItem, i){\n                accordionListItem.removeAttribute('open');\n            });\n        }\n    }\n<\/script>\n\n\n\n<h3>Ethical treatment of animals<\/h3>\n\n\n\n<p>Our company is dedicated to the ethical and responsible treatment of all animals used in the development of medicines and vaccines.<\/p>\n\n\n\n<p><a class=\"btn\" href=\"https:\/\/www.merck.com\/research-and-products\/discovery-development\/ethical-treatment-of-animals\/\">Learn more<\/a><\/p>\n\n\n<div id=\"mco-featured-banner-block-block_5e9f4997966ab\" class=\"mco-featured-banner-block full corporateberg-block negative\">\n            <div class=\"mco-featured-banner-image-container\">\n            <picture>\n                <source media=\"(max-width: 767px)\" data-srcset=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2020\/04\/Scientists-RY-3D-Tech-Model-of-lung-0539-1-HI_13_high.jpg?resize=767,956\">\n                <source media=\"(min-width: 768px) and (max-width: 1279px)\" data-srcset=\"https:\/\/www.merck.com\/wp-content\/uploads\/sites\/5\/2020\/04\/Scientists-RY-3D-Tech-Model-of-lung-0539-1-HI_13_high.jpg?resize=768,448\">\n                <source media=\"(min-width: 1280px)\" data-srcset=\"https:\/\/www.merck